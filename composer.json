{
    "name": "highliuk/wordpress-command",
    "description": "Easy way to define custom commands in WordPress powered by Symfony Console instead of WP-CLI",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "wordpress",
        "symfony",
        "console",
        "command",
        "wp-cli"
    ],
    "bin": [
        "bin/console"
    ],
    "autoload": {
        "psr-4": {
            "HighLiuk\\WordPressCommand\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Luca Di Fazio",
            "email": "highliuk@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/console": "^6.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.11",
        "laravel/pint": "^1.16",
        "szepeviktor/phpstan-wordpress": "^1.3",
        "phpstan/extension-installer": "^1.4"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
